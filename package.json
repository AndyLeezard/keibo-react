{
  "name": "keibo-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint-quiet": "eslint . --ext ts,tsx --quiet",
    "check": "tsc --skipLibCheck --noEmit && eslint . --ext ts,tsx --report-unused-disable-directives",
    "check-quiet": "tsc --skipLibCheck --noEmit && eslint . --ext ts,tsx --quiet",
    "pre-commit": "lint-staged",
    "preview": "vite preview",
    "postinstall": "husky install",
    "type-check": "tsc --skipLibCheck --noEmit",
    "bump": "node scripts/vbumper.exec.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
